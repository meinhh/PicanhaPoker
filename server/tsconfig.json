{
  "compilerOptions": {
	"module": "commonjs",
	"esModuleInterop": true,
	"target": "es2017",
	"noImplicitAny": true,
	"allowJs": false,
	"moduleResolution": "node",
	"sourceMap": true,
	"outDir": "dist",
	"baseUrl": "./src",
	"paths": {
	  "common/*": [
		"../../common/*"
	  ]
	}
  },
  "include": [
	"src/**/*"
  ]
}